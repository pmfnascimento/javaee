<?xml version="1.0" encoding="UTF-8" ?>  
<!DOCTYPE struts PUBLIC  
    "-//Apache Software Foundation//DTD Struts Configuration 2.0//EN"  
    "http://struts.apache.org/dtds/struts-2.0.dtd">  
<struts>  
	<constant name="struts.devMode" value="true" />
    <package name="WorldManagerWithStruts2" extends="struts-default">  
        <action name="Home">
        	<result>/index.jsp</result>
        </action>
        
        <action name="Login">
        	<result>/login.jsp</result>
        </action>
        <action name="LoginUser" class="com.worldmanager.actions.LoginUserAction">  
            <result name="success">index.jsp</result>  
            <result name="error">loginError.jsp</result>  
            <result name="input">login.jsp</result>
        </action>  
    	
    	<action name="SignOut">
    		<result>/signout.do</result>
    	</action>
    	
    	<action name="ClearUserData">
    		<result>/invalidatesessionandremovecookies.do</result>
    	</action>

    </package>  
</struts>  